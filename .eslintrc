{
  "env": {
      "browser": true,
      "es6": true,
      "node": true,
      "jest": true
  },
  "extends": [
      "airbnb",
      "eslint:recommended",
      "plugin:import/errors",
      "plugin:import/warnings"
    ],
    "plugins": [
      "react"
    ],
  "parser": "babel-eslint",
  "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true,
        "modules": true
      }
    },
    "globals": {
      "window": true
    }
}
